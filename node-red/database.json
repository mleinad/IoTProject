        "type": "template",
        "z": "c6eaed35c6501895",
        "name": "Query",
        "field": "topic",
        "fieldType": "msg",
        "syntax": "mustache",
        "template": "SELECT \n    DATE(charging_start_time) as date,\n    COUNT(*) as sessions,\n    ROUND(SUM(energy_consumed_kwh), 2) as energy\nFROM ev_charging_data\nGROUP BY DATE(charging_start_time)\nORDER BY date DESC\nLIMIT 10\n",
