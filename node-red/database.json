    {
        "id": "function1",
        "type": "function",
        "z": "c6eaed35c6501895",
        "name": "Format",
        "func": "var dates = [];\nvar sessions = [];\nvar costs = [];\n\nmsg.payload.forEach(row => {\n    dates.push(row.date);\n    sessions.push(row.sessions);\n    costs.push(row.avg_cost);\n});\n\nmsg.payload = [{\n    series: ['Sessions', 'Avg Cost'],\n    data: [sessions, costs],\n    labels: dates\n}];\nreturn msg;",
